<multistatus
    xmlns="DAV:">
    <response>
        <href>dcbox:/</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                </resourcetype>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/article_image</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                </resourcetype>
                <acl xml:base="https://demo.personium.io/fst-community-organization/__role/__/"
                    xmlns:p="urn:x-personium:xmlns">
                    <ace>
                        <principal>
                            <href>Organization</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                            <privilege>
                                <write/>
                            </privilege>
                        </grant>
                    </ace>
                    <ace>
                        <principal>
                            <href>User</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                        </grant>
                    </ace>
                </acl>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/reply</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                    <p:odata xmlns:p="urn:x-personium:xmlns"/>
                </resourcetype>
                <acl xml:base="https://demo.personium.io/fst-community-organization/__role/__/"
                    xmlns:p="urn:x-personium:xmlns">
                    <ace>
                        <principal>
                            <href>Organization</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                            <privilege>
                                <write/>
                            </privilege>
                        </grant>
                    </ace>
                    <ace>
                        <principal>
                            <href>User</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                            <privilege>
                                <write/>
                            </privilege>
                        </grant>
                    </ace>
                </acl>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/article</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                    <p:odata xmlns:p="urn:x-personium:xmlns"/>
                </resourcetype>
                <acl xml:base="https://demo.personium.io/app-life-enrichers-community/__role/__/"
                    xmlns:p="urn:x-personium:xmlns">
                    <ace>
                        <principal>
                            <href>Organization</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                            <privilege>
                                <write/>
                            </privilege>
                        </grant>
                    </ace>
                    <ace>
                        <principal>
                            <href>User</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                        </grant>
                    </ace>
                </acl>
            </prop>
        </propstat>
    </response>
</multistatus>
